@model Data.Models.demande

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_BackOfficeLayout.cshtml";
}



<div class="page-content-wrapper">
    <div class="page-content">

        <div class="page-bar">
            <div class="page-title-breadcrumb">
                <div class=" pull-left">
                    <div class="page-title">Dashboard</div>
                </div>
                <ol class="breadcrumb page-breadcrumb pull-right">
                    <li>
                        <i class="fa fa-home"></i>&nbsp;<a class="parent-item" href="index.html">Home</a>&nbsp;<i class="fa fa-angle-right"></i>
                    </li>
                    <li class="active">Dashboard</li>
                </ol>

            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card card-box">
                    <div class="card-head" style="padding-left:40px;">
                        <h3>Are you sure you want to delete this?</h3>
                        <div>
                            <hr />
                            <dl class="dl-horizontal">
                                <dt>
                                    @Html.DisplayNameFor(model => model.email) : @ViewBag.email
                                </dt>

                                

                                <dt>
                                    @Html.DisplayNameFor(model => model.firstName) : @ViewBag.firstName
                                </dt>

                                

                                <dt>
                                    @Html.DisplayNameFor(model => model.lastName) : @ViewBag.lastName 
                                </dt>

                                

                                <dt>
                                    @Html.DisplayNameFor(model => model.speciality) : @ViewBag.speciality
                                </dt>

                                

                                <dt>
                                    @Html.DisplayNameFor(model => model.state) : @ViewBag.state 
                                </dt>

                               

                            </dl>

                            @using (Html.BeginForm())
            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <input type="submit" value="Delete" class="btn btn-default" /> |
                                    @Html.ActionLink("Back to List", "Index")
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

